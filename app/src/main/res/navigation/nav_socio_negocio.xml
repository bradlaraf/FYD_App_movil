<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_socio_negocio"
    app:startDestination="@id/clientesFragment">

    <fragment
        android:id="@+id/clientesFragment"
        android:name="com.mobile.massiveapp.ui.view.sociodenegocio.fragments.ClientesFragment"
        android:label="fragment_clientes"
        tools:layout="@layout/fragment_clientes" >
        <action
            android:id="@+id/action_clientesFragment_to_leadFragment"
            app:destination="@id/leadFragment" />
    </fragment>
    <fragment
        android:id="@+id/leadFragment"
        android:name="com.mobile.massiveapp.ui.view.sociodenegocio.fragments.LeadFragment"
        android:label="fragment_lead"
        tools:layout="@layout/fragment_lead" >
        <action
            android:id="@+id/action_leadFragment_to_clientesFragment"
            app:destination="@id/clientesFragment" />
    </fragment>
</navigation>